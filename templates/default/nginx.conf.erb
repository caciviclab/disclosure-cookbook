upstream disclosurebackend {
  server 127.0.0.1:8080 fail_timeout=0;
}

server {
  listen 80;
  server_name opencal.opendisclosure.io disclosure.caciviclab.org;

  location / {
    include uwsgi_params;
    uwsgi_pass disclosurebackend;
  }

  location /static {
    #alias /data/backend/static;
    alias /data/backend/current/disclosure-backend/static;
  }
}
